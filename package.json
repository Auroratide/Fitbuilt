  "main": "dist/index.js",
  "scripts": {
    "start": "node .",
    "start:watch": "npm run build && npm-run-all --parallel start 'build -- --watch'",
    "clean": "rm -rf dist",
    "build": "webpack",
    "lint": "eslint '{server,client}/**/*.{js,ts,svelte}'",
    "pretest": "npm run lint",
    "test": "npm-run-all test:client test:server",
    "test:client": "jest client",
    "test:server": "jest server"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/svelte": "^1.7.1",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.17",
    "@types/node": "^12.7.1",
    "@types/node-fetch": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-jest": "^24.8.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "eslint": "^6.1.0",
    "eslint-plugin-jest": "^22.15.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "fetch-mock": "^7.3.9",
    "jest": "^24.8.0",
    "jest-transform-svelte": "^2.0.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "svelte": "^3.7.1",
    "svelte-loader": "^2.13.6",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>jest-setup.js"
    ],
    "testRegex": ".*\\.example\\.[jt]sx?$",
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "ts-jest",
      "^.+\\.svelte$": "jest-transform-svelte"
    }
  }
}
